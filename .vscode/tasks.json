{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Debug chess ai",
            "command": "C:\\msys64\\mingw64\\bin\\g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/src/*.cpp",
                "-I",
                "include",
                "-I",
                "include/sdl2",
                "-I",
                "include/headers",
                "-L",
                "${workspaceFolder}/lib",
                "-o",
                "Main",
                "-l",
                "mingw32",
                "-lSDL2main",
                "-lSDL2",
                "-lSDL2_image",
